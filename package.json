{
  "name": "gherkin-feature-csv-transform",
  "displayName": "gherkin-feature-csv-transform",
  "description": "",
  "version": "0.12.0",
  "repository": {
    "type": "github",
    "url": "https://github.com/jacksoncougar/gherkin-feature-csv-transform.git"
  },
  "publisher": "JacksonCWiebe",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:gherkin-feature-csv-transform.exportToZephyrCsv"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gherkin-feature-csv-transform.exportToZephyrCsv",
        "title": "Gherkin: Export To Zephyr Scale (Csv)",
        "enablement": "resourceLangId == Gherkin || resourceLangId == feature"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.59.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "csv-stringify": "^5.6.2",
    "gray-matter": "^4.0.3"
  }
}
